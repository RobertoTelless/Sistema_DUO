@using Presentation.Extensions
@model SMS_Solution.ViewModels.ContaBancariaViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="modalContainer">
        <div id="modal" class="modal" tabindex="-1" role="dialog">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        @*titulo gerado dinamicamente*@
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        @*Gerado dinamicamente*@
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="page-title">
        <h3 class="title">Bancos & Contas Bancárias</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaBanco", "Banco")">Bancos</a>
            </li>
            <li>
                <a href="@Url.Action("VoltarAnexoBanco", "Banco")">Bancos - Consulta & Edição</a>
            </li>
            <li class="active">
                <strong>Contas Bancárias - Consulta / Edição</strong>
            </li>
        </ol>
        <br />
    </div>

     <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Filtro (Lançamentos)</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Config option 1</a>
                            </li>
                            <li>
                                <a href="#">Config option 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>

                @using (Html.BeginForm("FiltrarLancamento", "Banco", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal" }))
                {
                    <div class="ibox-content">
                        @Html.AntiForgeryToken()
                        @if (!ViewData.ModelState.IsValid)
                        {
                            <div class="alert alert-danger">
                                <button type="button" class="close" data-dismiss="alert">Fechar</button>
                                @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                            </div>
                        }
                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Tipo
                                @Html.DropDownList("CBLA_IN_TIPO", (SelectList)ViewBag.TipoLanc, "Selecionar", new { @class = "form-control", autofocus = true })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                @Html.HiddenFor(m => m.COBA_CD_ID)
                                Data
                                @Html.TextBox("CBLA_DT_LANCAMENTO", null, new { @class = "form-control date-picker", autofocus = true })
                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                Descrição
                                @Html.TextBox("CBLA_DS_DESCRICAO", null, new { @class = "form-control", autofocus = true })
                            </div>
                        </div>
                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                            <div class="col-lg-12 col-md-12 col-xs-12">
                                @Html.ButtonActionSubmit(text: "Filtrar")
                                @Html.ButtonLinkIndex(text: "Retirar Filtro", controller: "Banco", action: "RetirarFiltroLancamento")
                                @Html.ButtonLinkIndex(text: "Incluir Novo Lançamento", action: "IncluirLancamento", controller: "Banco")
                                @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarBaseConta", controller: "Banco")
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Contas Bancárias - Consulta / Edição</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                @using (Html.BeginForm("EditarConta", "Banco", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                {
                    <div class="ibox-content">

                        <div class="tabs-container">
                            <ul class="nav nav-tabs">
                                <li class="@ViewBag.TabDadosGer"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                                <li class="@ViewBag.TabLanc"><a data-toggle="tab" href="#tab-2">Lançamentos</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-3">Contatos</a></li>
                            </ul>

                            <div class="tab-content">

                                @*Dados Gerais*@
                                <div id="tab-1" class="tab-pane @ViewBag.TabDadosGer">
                                    <div class="panel-body">
                                        <div class="form-group">
                                            <div class="col-lg-4 col-md-4 col-xs-12">
                                                @Html.HiddenFor(m => m.COBA_CD_ID)
                                                @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                @Html.HiddenFor(m => m.BANC_CD_ID)
                                                @Html.HiddenFor(m => m.COBA_IN_ATIVO)
                                                @Html.HiddenFor(m => m.COBA_VL_SALDO_ATUAL)
                                                @Html.HiddenFor(m => m.COBA_VL_SALDO_INICIAL)
                                                @Html.HiddenFor(m => m.TICO_CD_ID)
                                                Tipo de Conta*
                                                @Html.TextBoxFor(m => m.TIPO_CONTA.TICO_NM_NOME, new { @class = "form-control", @readonly = "readonly" })
                                                @Html.ValidationMessageFor(m => m.TICO_CD_ID, "", new { @style = "color:red" })
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                Num.Agência*
                                                @Html.TextBoxFor(m => m.COBA_NR_AGENCIA, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                @Html.ValidationMessageFor(m => m.COBA_NR_AGENCIA, "", new { @style = "color:red" })
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                Nome da Agência
                                                @Html.TextBoxFor(m => m.COBA_NM_AGENCIA, new { @class = "form-control", autofocus = true })
                                                @Html.ValidationMessageFor(m => m.COBA_NM_AGENCIA, "", new { @style = "color:red" })
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                Número da Conta*
                                                @Html.TextBoxFor(m => m.COBA_NR_CONTA, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                @Html.ValidationMessageFor(m => m.COBA_NR_CONTA, "", new { @style = "color:red" })
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                Data de Abertura*
                                                @Html.TextBoxFor(m => m.COBA_DT_ABERTURA, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true })
                                                @Html.ValidationMessageFor(m => m.COBA_DT_ABERTURA, "", new { @style = "color:red" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-4 col-md-4 col-xs-12">
                                                Nome da Conta
                                                @Html.TextBoxFor(m => m.COBA_NM_NOME, new { @class = "form-control", autofocus = true })
                                                @Html.ValidationMessageFor(m => m.COBA_NM_NOME, "", new { @style = "color:red" })
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                Saldo Inicial
                                                @Html.TextBoxFor(m => m.COBA_VL_SALDO_INICIAL, "{0:n2}", new { @class = "form-control", autofocus = true, @readonly = "readonly", @style = "color:green; font-weight:bold" })
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                Saldo Atual
                                                @if (Model.COBA_VL_SALDO_ATUAL < 0)
                                                {
                                                    @Html.TextBoxFor(m => m.COBA_VL_SALDO_ATUAL, "{0:n2}", new { @class = "form-control", autofocus = true, @readonly = "readonly", @style = "color:red; font-weight:bold" })
                                                }
                                                else
                                                {
                                                    @Html.TextBoxFor(m => m.COBA_VL_SALDO_ATUAL, "{0:n2}", new { @class = "form-control", autofocus = true, @readonly = "readonly", @style = "color:blue; font-weight:bold" })
                                                }
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-4 col-md-4 col-xs-12">
                                                Nome do Gerente
                                                @Html.TextBoxFor(m => m.COBA_NM_GERENTE, new { @class = "form-control", autofocus = true })
                                                @Html.ValidationMessageFor(m => m.COBA_NM_GERENTE, "", new { @style = "color:red" })
                                            </div>
                                            <div class="col-lg-4 col-md-4 col-xs-12">
                                                Telefones
                                                @Html.TextBoxFor(m => m.COBA_NR_TELEFONE, new { @class = "form-control", autofocus = true, placeholder = "(99) 9999-9999 R: 99", @id = "tel" })
                                                @Html.ValidationMessageFor(m => m.COBA_NR_TELEFONE, "", new { @style = "color:red" })
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                <div class="input-group">
                                                    Conta Padrão
                                                    <br />
                                                    @Html.CheckBoxFor(m => m.Padrao, new { @class = "checkbox i-checks", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.Padrao, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                @Html.ButtonActionSubmit(text: "Confirma Edição", htmlAttributes: new { id = "basictoast" })
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Lançamentos*@
                                <div id="tab-2" class="tab-pane @ViewBag.TabLanc">
                                    <div class="panel-body">
                                        <div class="form-group">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Lançamentos</h4>
                                                <br />
                                                @if (Model.CONTA_BANCO_LANCAMENTO != null)
                                                {
                                                    if (Model.CONTA_BANCO_LANCAMENTO.Count > 0)
                                                    {
                                                        <table class="table table-striped table-bordered table-hover dataTables-example">
                                                            <thead style="background-color:lightsteelblue">
                                                                <tr>
                                                                    <th>Data</th>
                                                                    <th>Número</th>
                                                                    <th>Tipo</th>
                                                                    <th>Descrição</th>
                                                                    <th>Valor</th>
                                                                    <th>Ativo</th>
                                                                    <th class="td-one-action">Ações</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var cond in (Model.CONTA_BANCO_LANCAMENTO))
                                                                {
                                                                    if (cond.CBLA_IN_ATIVO == 1)
                                                                    {
                                                                        <tr>
                                                                            <td>@cond.CBLA_DT_LANCAMENTO.Value.ToShortDateString()</td>
                                                                            <td>@cond.CBLA_NR_NUMERO</td>
                                                                            @if (cond.CBLA_IN_TIPO == 1)
                                                                            {
                                                                                <td>C</td>
                                                                                <td>@cond.CBLA_DS_DESCRICAO</td>
                                                                                if (cond.CBLA_VL_VALOR != null)
                                                                                {
                                                                                    <td style="color: darkgreen">@CrossCutting.Formatters.DecimalFormatter(cond.CBLA_VL_VALOR.Value)</td>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <td style="color: darkgreen">0,00</td>
                                                                                }
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>D</td>
                                                                                <td>@cond.CBLA_DS_DESCRICAO</td>
                                                                                if (cond.CBLA_VL_VALOR != null)
                                                                                {
                                                                                    <td style="color: red">- @CrossCutting.Formatters.DecimalFormatter(cond.CBLA_VL_VALOR.Value)</td>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <td style="color: red">0,00</td>
                                                                                }
                                                                            }
                                                                            @if (cond.CBLA_IN_ATIVO == 1)
                                                                            {
                                                                                <td>Ativo</td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>Inativo</td>
                                                                            }
                                                                            <td class="td-one-action">
                                                                                @Html.TableLinkDetails(cond.CBLA_CD_ID, "EditarLancamento", "fa-edit", "Editar/Consultar")
                                                                                @if (cond.CBLA_IN_ORIGEM == 1)
                                                                                {
                                                                                    if (cond.CBLA_IN_ATIVO == 1)
                                                                                    {
                                                                                        <span style="color: #337ab7; cursor:pointer;" alt="Excluir" class="tbl-link fa-lg fa fa-trash-o" onclick="ShowModalExcluir('/Banco/ExcluirLancamento/@cond.CBLA_CD_ID')" title="Excluir"></span>
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        @Html.TableLinkDetails(cond.CBLA_CD_ID, "ReativarLancamento", "fa-refresh", "Reativar")
                                                                                    }
                                                                                }
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tr style="background-color: burlywood">
                                                                            <td>@cond.CBLA_DT_LANCAMENTO.Value.ToShortDateString()</td>
                                                                            <td>@cond.CBLA_NR_NUMERO</td>
                                                                            @if (cond.CBLA_IN_TIPO == 1)
                                                                            {
                                                                                <td>C</td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>D</td>
                                                                            }
                                                                            <td>@cond.CBLA_DS_DESCRICAO</td>
                                                                            @if (cond.CBLA_VL_VALOR != null)
                                                                            {
                                                                                <td>@CrossCutting.Formatters.DecimalFormatter(cond.CBLA_VL_VALOR.Value)</td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>0,00</td>
                                                                            }
                                                                            @if (cond.CBLA_IN_ATIVO == 1)
                                                                            {
                                                                                <td>Ativo</td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>Inativo</td>
                                                                            }
                                                                            <td class="td-one-action">
                                                                                @Html.TableLinkDetails(cond.CBLA_CD_ID, "EditarLancamento", "fa-edit", "Editar/Consultar")
                                                                                @if (cond.CBLA_IN_ORIGEM == 1)
                                                                                {
                                                                                    if (cond.CBLA_IN_ATIVO == 1)
                                                                                    {
                                                                                        <span style="color: #337ab7; cursor:pointer;" alt="Excluir" class="tbl-link fa-lg fa fa-trash-o" onclick="ShowModalExcluir('/Banco/ExcluirLancamento/@cond.CBLA_CD_ID')" title="Excluir"></span>
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <span style="color: #337ab7; cursor:pointer;" alt="Reativar" class="tbl-link fa-lg fa fa-refresh" onclick="ShowModalReativar('/Banco/ReativarLancamento/@cond.CBLA_CD_ID')" title="Reativar"></span>
                                                                                    }
                                                                                }
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                }
                                                            </tbody>
                                                        </table>
                                                    }
                                                }
                                            </div>
                                        </div>
                                    <div class="form-group">
                                        <div class="col-lg-12 col-md-12 col-xs-12">
                                            @Html.ButtonLinkIndex(text: "Incluir Novo Lançamento", action: "IncluirLancamento", controller: "Banco")
                                        </div>
                                    </div>
                                    </div>
                                </div>

                                @*Contatos*@
                                <div id="tab-3" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="form-group">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Contatos</h4>
                                                <br />
                                                @if (Model.CONTA_BANCO_CONTATO != null)
                                                {
                                                    if (Model.CONTA_BANCO_CONTATO.Count > 0)
                                                    {
                                                        <table class="table table-striped table-bordered table-hover dataTables-example">
                                                            <thead style="background-color:lightsteelblue">
                                                                <tr>
                                                                    <th>Nome</th>
                                                                    <th>Cargo</th>
                                                                    <th>E-Mail</th>
                                                                    <th>Telefone</th>
                                                                    <th>Ativo</th>
                                                                    <th class="td-one-action">Ações</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var cond in (Model.CONTA_BANCO_CONTATO))
                                                                {
                                                                    if (cond.CBCT_IN_ATIVO == 1)
                                                                    {
                                                                        <tr>
                                                                            <td>@cond.CBCT_NM_NOME</td>
                                                                            <td>@cond.CBCT_NM_CARGO</td>
                                                                            <td>@cond.CBCT_NM_EMAIL</td>
                                                                            <td>@cond.CBCT_NR_TELEFONES</td>
                                                                            @if (cond.CBCT_IN_ATIVO == 1)
                                                                            {
                                                                                <td>Sim</td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>Não</td>
                                                                            }
                                                                            <td class="td-one-action">
                                                                                @Html.TableLinkDetails(cond.CBCT_CD_ID, "EditarContato", "fa-edit", "Editar/Consultar")
                                                                                @if (cond.CBCT_IN_ATIVO == 1)
                                                                                {
                                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Excluir" class="tbl-link fa-lg fa fa-trash-o" onclick="ShowModalExcluir('/Banco/ExcluirContato/@cond.CBCT_CD_ID')" title="Excluir"></span>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Reativar" class="tbl-link fa-lg fa fa-refresh" onclick="ShowModalReativar('/Banco/ReativarContato/@cond.CBCT_CD_ID')" title="Reativar"></span>
                                                                                }
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tr style="background-color: burlywood">
                                                                            <td>@cond.CBCT_NM_NOME</td>
                                                                            <td>@cond.CBCT_NM_CARGO</td>
                                                                            <td>@cond.CBCT_NM_EMAIL</td>
                                                                            <td>@cond.CBCT_NR_TELEFONES</td>
                                                                            @if (cond.CBCT_IN_ATIVO == 1)
                                                                            {
                                                                                <td>Sim</td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>Não</td>
                                                                            }
                                                                            <td class="td-one-action">
                                                                                @Html.TableLinkDetails(cond.CBCT_CD_ID, "EditarContato", "fa-edit", "Editar/Consultar")
                                                                                @if (cond.CBCT_IN_ATIVO == 1)
                                                                                {
                                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Excluir" class="tbl-link fa-lg fa fa-trash-o" onclick="ShowModalExcluir('/Banco/ExcluirContato/@cond.CBCT_CD_ID')" title="Excluir"></span>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Reativar" class="tbl-link fa-lg fa fa-refresh" onclick="ShowModalReativar('/Banco/ReativarContato/@cond.CBCT_CD_ID')" title="Reativar"></span>
                                                                                }
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                }
                                                            </tbody>
                                                        </table>
                                                    }
                                                }
                                            </div>
                                        </div>
                                    <div class="form-group">
                                        <div class="col-lg-12 col-md-12 col-xs-12">
                                            @Html.ButtonLinkIndex(text: "Incluir Novo Contato", action: "IncluirContato", controller: "Banco")
                                        </div>
                                    </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <br />
                                    <div class="col-lg-12 col-md-12 col-xs-12">
                                        @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarBaseConta", controller: "Banco")
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>

@section Styles {
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/themes/base/all.css" rel="stylesheet" />
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/plugins/jasnyBootstrapStyles")
    @Styles.Render("~/Content/plugins/iCheck/iCheckStyles")
    @Styles.Render("~/plugins/toastrStyles")
    @Styles.Render("~/plugins/touchSpinStyles")
    @Styles.Render("~/plugins/sweetAlertStyles")
}
@section Scripts {
    @Scripts.Render("~/plugins/toastr")
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/bundles/inputmask")
    @Scripts.Render("~/plugins/jasnyBootstrap")
    @Scripts.Render("~/plugins/touchSpin")
    @Scripts.Render("~/plugins/iCheck")
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.maskedinput/1.4.1/jquery.maskedinput.js"></script>

        <script>
        $(function () {
            limpaModal();
        });

        function limpaModal() {
            $('#btnModal').remove();
            $('#modalTitle').remove();
        }

        function ShowModalExcluir(url) {
            $('#modal').find('.modal-footer').prepend('<a id="btnModal" alt="Desativar" class="btn btn-primary" href="' + url + '" target="_self" title="Desativar">Confirmar</a>');
            $('#modal').find('.modal-body').html('Confirma exclusão?');
            $('#modal').find('.modal-header').prepend('<h5 id="modalTitle" class="modal-title">Confimar Exclusão</h5>');
            $('#modal').modal('show');
        }

        function ShowModalReativar(url) {
