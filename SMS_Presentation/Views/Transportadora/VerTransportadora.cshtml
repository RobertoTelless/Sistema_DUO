    <script>
        function PesquisaCep(cep, btn, tipoEnd) {
            data = {
                cep: cep
                , tipoEnd: tipoEnd
            }

            $('#btnBuscaCep').attr("disabled", "disabled");

            $.ajax({
                url: '../Transportadora/PesquisaCEP_Javascript'
                , type: 'POST'
                , data: data
                , success: function (r) {
                    $('#endereco').val(r.TRAN_NM_ENDERECO);
                    $('#bairro').val(r.TRAN_NM_BAIRRO);
                    $('#cidade').val(r.TRAN_NM_CIDADE);
                    $('#uf').val(r.TRAN_SG_UF);
                    $('#btnBuscaCep').removeAttr("disabled");
                }
            });
        }
    </script>

