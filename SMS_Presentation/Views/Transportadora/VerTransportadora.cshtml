@using Presentation.Extensions
@model SMS_Solution.ViewModels.TransportadoraViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">

    @using (Html.BeginForm("EnviarSmsTrans", "Transportadora", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal" }))
    { 
            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Enviar SMS</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                      @Html.Hidden("id", null, new { id = "idTrans" })
                      Mensagem
                      @Html.TextBox("mensagem", null, new { @style = "height: 200px;", @class = "form-control" })
                  </div>
                  <div class="modal-footer">
                      @Html.ButtonActionSubmit("Enviar")
                    <button type="button" class="btn btn-sm btn-warning" data-dismiss="modal">Cancelar</button>
                  </div>
                </div>
              </div>
            </div>
    }

    <div class="page-title">
        <h3 class="title">Transportadoras</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaTransportadora", "Transportadora")">Transportadoras</a>
            </li>
            <li class="active">
                <strong>Transportadora - Consulta</strong>
            </li>
        </ol>
        <br />
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Transportadora - Consulta</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>

                <div class="ibox-content">
                    <div class="form-group">
                        <span class="btn btn-sm btn-warning" onclick="MudaIdTrans('@Model.TRAN_CD_ID')" data-toggle="modal" data-target="#exampleModal">Enviar SMS</span>
                        @Html.ButtonLinkIndex(text: "Gerar Relatório", action: "GerarRelatorioDetalhe", controller: "Transportadora")
                        @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarBaseTransportadora", controller: "Transportadora")
                    </div>
                    <div class="tabs-container">
                        <ul class="nav nav-tabs">
                            <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-2">Anexos & Logotipo</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-3">Endereços</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-4">Contatos</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-5">Observações</a></li>
                        </ul>
