@using Presentation.Extensions
@model SMS_Solution.ViewModels.ContaReceberViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="page-title">
        <h3 class="title">Contas a Receber</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaCR", "ContaReceber")">Contas a Receber</a>
            </li>
            <li class="active">
                <strong>Contas a Receber - Edição</strong>
            </li>
        </ol>
        <br />
    </div>

    <div class="row">
        @if (Model.CARE_IN_LIQUIDADA == 0)
        {
            <div class="col-lg-2">
                <div class="widget style1 blue-bg">
                    <div class="row vertical-align">
                        <div class="col-xs-3">
                            <i class="fa fa-thumbs-o-up fa-3x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            <h3 class="font-normal">Em aberto</h3>
                        </div>
                    </div>
                </div>
            </div>
        }
        else
        {
            <div class="col-lg-2">
                <div class="widget style1 yellow-bg">
                    <div class="row vertical-align">
                        <div class="col-xs-3">
                            <i class="fa fa-check-square-o fa-3x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            <h3 class="font-normal">Liquidada</h3>
                        </div>
                    </div>
                </div>
            </div>
        }
        @if (Model.CARE_NR_ATRASO > 0)
        {
            <div class="col-lg-2">
                <div class="widget style1 red-bg">
                    <div class="row vertical-align">
                        <div class="col-xs-3">
                            <i class="fa fa-thumbs-o-up fa-3x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            <h3 class="font-normal">Atraso: @Model.CARE_NR_ATRASO</h3>
                        </div>
                    </div>
                </div>
            </div>
        }
        <div class="col-lg-2">
            <div class="widget style1 navy-bg">
                <div class="row vertical-align">
                    <div class="col-xs-3">
                        <i class="fa fa-thumbs-o-up fa-3x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <h3 class="font-normal">Vencimento: @Model.CARE_DT_VENCIMENTO.Value.ToShortDateString()</h3>
                    </div>
                </div>
            </div>
        </div>
    </div>

