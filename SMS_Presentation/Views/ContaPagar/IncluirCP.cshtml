@using Presentation.Extensions
@model SMS_Solution.ViewModels.ContaPagarViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="page-title">
        <h3 class="title">Contas a Pagar</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaCP", "ContaPagar")">Contas a Pagar</a>
            </li>
            <li class="active">
                <strong>Contas a Pagar - Inclusão</strong>
            </li>
        </ol>
        <br />
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Conta a Pagar - Inclusão</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>

                <div class="ibox-content">
                    @Html.AntiForgeryToken()
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                    <div class="tabs-container">
                        @using (Html.BeginForm("IncluirCP", "ContaPagar", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                        {
                            <div class="form-group">
                                <div class="col-lg-12 col-md-12 col-xs-12">
                                    <button class="btnSubmit btn btn-sm btn-success" id="basictoast" type="button">Confirma Inclusão de Lançamento</button>
                                    @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarBaseCP", controller: "ContaPagar")
                                </div>
                            </div>

                            <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais e Parcelamento</a></li>
                                <li><a data-toggle="tab" href="#tab-2">Anexos</a></li>
                            </ul>
                            <div class="tab-content">
                                @*Dados Gerais e Parcelamento*@
                                <div id="tab-1" class="tab-pane active">
                                    <div class="panel-body">
                                        <div class="row">
                                            <h4>Dados Gerais</h4>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    @Html.HiddenFor(m => m.CAPA_IN_ATIVO)
                                                    @Html.HiddenFor(m => m.CAPA_IN_LIQUIDADA)
                                                    @Html.HiddenFor(m => m.CAPA_IN_PAGA_PARCIAL)
                                                    @Html.HiddenFor(m => m.CAPA_IN_PARCELADA)
                                                    @Html.HiddenFor(m => m.CAPA_IN_TIPO_LANCAMENTO)
                                                    @Html.HiddenFor(m => m.USUA_CD_ID)
                                                    @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                    Fornecedor
                                                    <div class="input-group">
                                                        <select name="FORN_CD_ID" id="lstForn" class="form-control" data-val="true" style="display: none;" autofocus>
                                                            <option>Selecionar</option>
                                                        </select>
                                                        <span class="input-group-addon">
                                                            @Html.TableLinkAction("IncluirFornecedor", "ContaPagar", null, "glyphicon glyphicon-plus", "Adicionar Fornecedor")
                                                        </span>
                                                    </div>
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Centro de Custo
                                                    @Html.DropDownListFor(m => m.CECU_CD_ID, (SelectList)ViewBag.CC, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CECU_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Responsável
                                                    @Html.DropDownListFor(m => m.USUA_CD_ID, (SelectList)ViewBag.Usuarios, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.USUA_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Num.Documento
                                                    @Html.TextBoxFor(m => m.CAPA_NR_DOCUMENTO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_NR_DOCUMENTO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Forma de Pagamento*
                                                    @Html.DropDownListFor(m => m.FOPA_CD_ID, (SelectList)ViewBag.Formas, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FOPA_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Tipo de Documento
                                                    @Html.DropDownListFor(m => m.CAPA_IN_TIPO_DOC, (SelectList)ViewBag.TipoDoc, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_IN_TIPO_DOC, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <hr />
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Descrição
                                                    @Html.TextBoxFor(m => m.CAPA_DS_DESCRICAO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_DS_DESCRICAO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data de Emissão
                                                    @Html.TextBoxFor(m => m.CAPA_DT_LANCAMENTO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_DT_LANCAMENTO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Competência
                                                    @Html.TextBoxFor(m => m.CAPA_DT_COMPETENCIA, "{0:MM/yyyy}", new { @class = "form-control date-picker1", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_DT_COMPETENCIA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Valor
                                                    @Html.TextBoxFor(m => m.CAPA_VL_VALOR, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_VL_VALOR, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data de Vencimento
                                                    @Html.TextBoxFor(m => m.CAPA_DT_VENCIMENTO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_DT_VENCIMENTO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <hr />
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Parcelamento/Pagamento Recorrente
                                                    @Html.DropDownListFor(m => m.CAPA_IN_PAGAMENTO, (SelectList)ViewBag.Pagamento, "Único", new { @class = "form-control", autofocus = true, onchange = "ChangePag($(this).val())", @id = "pagamento" })
                                                    @Html.ValidationMessageFor(m => m.CAPA_IN_PAGAMENTO, "", new { @style = "color:red" })
                                                </div>
                                                <div id="pagRecorrente">
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Num.Pagamentos
                                                        @Html.TextBoxFor(m => m.CAPA_IN_RECORRENTE, new { @class = "form-control touchspin3", autofocus = true, @id = "inRecorrente" })
                                                        @Html.ValidationMessageFor(m => m.CAPA_IN_RECORRENTE, "", new { @style = "color:red" })
                                                    </div>
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Data inicial
                                                        @Html.TextBoxFor(m => m.CAPA_DT_INICIO_RECORRENCIA, "{0:dd/MM/yyyy}", new { @class = "form-control", autofocus = true, @id = "data4" })
                                                        @Html.ValidationMessageFor(m => m.CAPA_DT_INICIO_RECORRENCIA, "", new { @style = "color:red" })
                                                    </div>
                                                </div>
                                                <div id="parcelamento">
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Num.Parcelas
                                                        @Html.TextBoxFor(m => m.CAPA_IN_PARCELAS, new { @class = "form-control touchspin2", autofocus = true, @id = "parcelas", onchange = "CheckNumParc($(this).val())" })
                                                        @Html.ValidationMessageFor(m => m.CAPA_IN_PARCELAS, "", new { @style = "color:red" })
                                                    </div>
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Data inicial
                                                        @Html.TextBoxFor(m => m.CAPA_DT_INICIO_PARCELAS, "{0:dd/MM/yyyy}", new { @class = "form-control", autofocus = true, @id = "data2" })
                                                        @Html.ValidationMessageFor(m => m.CAPA_DT_INICIO_PARCELAS, "", new { @style = "color:red" })
                                                    </div>
                                                </div>
                                                <div id="periodicidade">
                                                    <div class="col-lg-3 col-md-3 col-xs-12">
                                                        Periodicidade
                                                        @Html.DropDownListFor(m => m.PERI_CD_ID, (SelectList)ViewBag.Periodicidade, "Selecionar", new { @class = "form-control", autofocus = true })
                                                        @Html.ValidationMessageFor(m => m.PERI_CD_ID, "", new { @style = "color:red" })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <hr />
                                            <h4>Observações</h4>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Observações
                                                    @Html.TextAreaFor(m => m.CAPA_TX_OBSERVACOES, new { @class = "form-control", style = "width: 100%; height: 100px;", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_TX_OBSERVACOES, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Imagens - Foto e Anexos*@
                                <div id="tab-2" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <hr />
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Anexos</h4><input class="btn btn-sm btn-warning" id="inputFile" type="file" multiple />
                                                <br />
                                                <div class="form-group">
                                                    <div class="ibox-content">
                                                        <br />
                                                        <table style="width: 100%;" class="table table-striped table-bordered table-hover dataTables-example" id="myFiles">
                                                            <thead style="background-color:lightsteelblue">
                                                                <tr>
                                                                    <td>Nome</td>
                                                                    <td>Tipo</td>
                                                                    <td>Ações</td>
                                                                </tr>
                                                            </thead>
                                                            <tbody></tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <br />
                                <div class="col-lg-12 col-md-12 col-xs-12">
                                    @Html.ButtonActionSubmit(text: "BtnSubmit", htmlAttributes: new { id = "submit", style = "display: none;" })
                                    <button class="btnSubmit btn btn-sm btn-success" id="basictoast" type="button">Confirma Inclusão de Lançamento</button>
                                    @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarBaseCP", controller: "ContaPagar")
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

