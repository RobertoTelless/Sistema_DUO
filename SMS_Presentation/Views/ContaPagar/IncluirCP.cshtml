        $("#lstForn").select2({
            //data: filterSelect,
            ajax: {
                url: '../Fornecedor/BuscaNomeRazao',
                type: 'POST',
                data: function (params) {
                    var query = {
                        nome: params.term
                    }
                    return query
                },
                processResults: function (data) {
                    console.log(data);
                    return {
                        results: data
                    }
                }
            },
            placeholder: 'Busque por NOME ou RAZÃO SOCIAL',
            minimumInputLength: 1
        });
    </script>

    <script>
        function ChangePag(val) {
            if (val == 1) {
                $('#pagRecorrente').show();
                $('#parcelamento').hide();
                $('#periodicidade').show();

                $('#parcelas').val(0);
                $('#data2').val('');
            } else if (val == 2) {
                $('#pagRecorrente').hide();
                $('#parcelamento').show();
                $('#periodicidade').show();

                $('#parcelas').val(2);

                $('#inRecorrente').val(0);
                $('#data4').val('');
            } else {
                $('#pagRecorrente').hide();
                $('#parcelamento').hide();
                $('#periodicidade').hide();

                $('#parcelas').val(0);
                $('#data2').val('');
                $('#inRecorrente').val(0);
                $('#data4').val('');
            }
        }

        $(function () {
            ChangePag($('#pagamento').val());
        });

        function CheckNumParc(val) {
            if (val == 1) {
                $('#parcelas').val(2);
            }
        }
    </script>

