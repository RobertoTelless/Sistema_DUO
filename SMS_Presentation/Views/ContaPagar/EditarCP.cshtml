@using Presentation.Extensions
@model SMS_Solution.ViewModels.ContaPagarViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="page-title"> 
        <h3 class="title">Contas a Pagar</h3> 
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaCP", "ContaPagar")">Contas a Pagar</a>
            </li>
            <li class="active">
                <strong>Contas a Pagar - Edição</strong>
            </li>
        </ol>
        <br />
    </div>

    <div class="row">
        <div class="col-lg-2">
            <div class="widget style1 yellow-bg">
                <div class="row vertical-align">
                    <div class="col-xs-3">
                        <i class="fa fa-thumbs-o-up fa-3x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        @if (Model.CAPA_IN_LIQUIDADA == 0)
	                    {
                            <h3 class="font-normal">Em Aberto</h3>
                        }
                        else
                        {
                            <h3 class="font-normal">Liquidada</h3>
                        }
                    </div>
                </div>
            </div>
        </div>
        @if (Model.CAPA_NR_ATRASO > 0)
        {
            <div class="col-lg-2">
                <div class="widget style1 red-bg">
                    <div class="row vertical-align">
                        <div class="col-xs-3">
                            <i class="fa fa-thumbs-o-up fa-3x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            <h3 class="font-normal">Atraso: @Model.CAPA_NR_ATRASO</h3>
                        </div>
                    </div>
                </div>
            </div>
        }
        <div class="col-lg-2">
            <div class="widget style1 navy-bg">
                <div class="row vertical-align">
                    <div class="col-xs-3">
                        <i class="fa fa-thumbs-o-up fa-3x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <h3 class="font-normal">Vencimento: @Model.CAPA_DT_VENCIMENTO.Value.ToShortDateString()</h3>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Conta a Pagar - Edição</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>

                <div class="ibox-content">
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                    <div class="form-group">
                        <br />
                        @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarBaseCP", controller: "ContaPagar")
                    </div>
                    <div class="tabs-container">
                        <ul class="nav nav-tabs">
                            <li id="geral" class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                            <li id="parcela" class=""><a data-toggle="tab" href="#tab-2">Parcelas</a></li>
                            <li id="liquidar" class=""><a data-toggle="tab" href="#tab-3">Liquidação</a></li>
                            @if (Model.CAPA_IN_PARCELADA == 0)
	                        {
                                <li class=""><a data-toggle="tab" href="#tab-4">Pagamento Parcial</a></li>
	                        }   
                            <li class=""><a data-toggle="tab" href="#tab-5">Rateio de Plano de Contas</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-6">Anexos</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-7">Observações</a></li>
                        </ul>
                        <div class="tab-content">

                            @*Dados Gerais*@
                            <div id="tab-1" class="tab-pane active">
                                <div class="panel-body">
                                    @using (Html.BeginForm("EditarCP", "ContaPagar", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                                    {
                                        <div class="form-group">
                                            @Html.ButtonLinkIndex(text: "Duplicar Lançamento", action: "DuplicarCP", controller: "ContaPagar", htmlAttributes: new { id = Model.CAPA_CD_ID })
                                            @Html.ButtonActionSubmit(text: "Confirma Edição de Lançamento", htmlAttributes: new { id = "basictoast" })
                                        </div>

                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    @Html.HiddenFor(m => m.CAPA_CD_ID)
                                                    @Html.HiddenFor(m => m.CAPA_DT_LIQUIDACAO)
                                                    @Html.HiddenFor(m => m.CAPA_DT_INICIO_PARCELAS)
                                                    @Html.HiddenFor(m => m.CAPA_IN_ATIVO)
                                                    @Html.HiddenFor(m => m.CAPA_IN_LIQUIDADA)
                                                    @Html.HiddenFor(m => m.CAPA_IN_PAGA_PARCIAL)
                                                    @Html.HiddenFor(m => m.CAPA_IN_PARCELADA)
                                                    @Html.HiddenFor(m => m.CAPA_IN_PARCELAS)
                                                    @Html.HiddenFor(m => m.CAPA_IN_TIPO_LANCAMENTO)
                                                    @Html.HiddenFor(m => m.CAPA_TX_OBSERVACOES)
                                                    @Html.HiddenFor(m => m.CAPA_VL_DESCONTO)
                                                    @Html.HiddenFor(m => m.CAPA_VL_JUROS)
                                                    @Html.HiddenFor(m => m.CAPA_VL_PARCELADO)
                                                    @Html.HiddenFor(m => m.CAPA_VL_TAXAS)
                                                    @Html.HiddenFor(m => m.CAPA_VL_VALOR_PAGO)
                                                    @Html.HiddenFor(m => m.PERI_CD_ID)
                                                    @Html.HiddenFor(m => m.USUA_CD_ID)
                                                    @Html.HiddenFor(m => m.CAPA_VL_PARCIAL)
                                                    @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                    Fornecedor
                                                    <div class="input-group">
                                                        <select name="FORN_CD_ID" id="lstForn" class="form-control" data-val="true" style="display: none;" autofocus>
                                                            <option>Selecionar</option>
                                                        </select>
                                                        <span class="input-group-addon">
                                                            @Html.TableLinkAction("IncluirFornecedor", "ContaPagar", null, "glyphicon glyphicon-plus", "Adicionar Fornecedor")
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Centro de Custos
                                                    @Html.DropDownListFor(m => m.CECU_CD_ID, (SelectList)ViewBag.CC, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CECU_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Responsável
                                                    @Html.DropDownListFor(m => m.USUA_CD_ID, (SelectList)ViewBag.Usuarios, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.USUA_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Forma de Pagamento*
                                                    @Html.DropDownListFor(m => m.FOPA_CD_ID, (SelectList)ViewBag.Formas, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FOPA_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Tipo de Documento
                                                    @Html.DropDownListFor(m => m.CAPA_IN_TIPO_DOC, (SelectList)ViewBag.TipoDoc, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_IN_TIPO_DOC, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <hr />
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Num.Documento
                                                    @Html.TextBoxFor(m => m.CAPA_NR_DOCUMENTO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_NR_DOCUMENTO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-10 col-md-10 col-xs-12">
                                                    Descrição
                                                    @Html.TextBoxFor(m => m.CAPA_DS_DESCRICAO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_DS_DESCRICAO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data de Emissão
                                                    @Html.TextBoxFor(m => m.CAPA_DT_LANCAMENTO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_DT_LANCAMENTO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Competência
                                                    @Html.TextBoxFor(m => m.CAPA_DT_COMPETENCIA, "{0:MM/yyyy}", new { @class = "form-control date-picker", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_DT_COMPETENCIA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Valor
                                                    @Html.TextBoxFor(m => m.CAPA_VL_VALOR, "{0:n2}", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CAPA_VL_VALOR, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data de Vencimento
                                                    @if (Model.Parcelado || Model.Parcial)
                                                    {
                                                        @Html.TextBoxFor(m => m.CAPA_DT_VENCIMENTO, "{0:dd/MM/yyyy}", new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                    }
                                                    else
                                                    {
                                                        @Html.TextBoxFor(m => m.CAPA_DT_VENCIMENTO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true })
                                                    }
                                                    @Html.ValidationMessageFor(m => m.CAPA_DT_VENCIMENTO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Saldo
                                                    @Html.TextBoxFor(m => m.CAPA_VL_SALDO, "{0:n2}", new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                                @if (Model.CAPA_NR_ATRASO > 0)
                                                {
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Atraso
                                                        @Html.TextBoxFor(m => m.CAPA_NR_ATRASO, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <br />
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                @Html.ButtonActionSubmit(text: "Confirma Edição de Lançamento", htmlAttributes: new { id = "basictoast" })
                                                @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarBaseCP", controller: "ContaPagar")
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>

                            @*Liquidação*@
                            <div id="tab-3" class="tab-pane">
                                <div class="panel-body">
                                    @using (Html.BeginForm("EditarCP", "ContaPagar", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id="pwd-container1" }))
                                    {
                                        if (ViewBag.TemSaldo == 0)
                                        {
                                            <div class="modal fade" id="liquidaModal" tabindex="-1" role="dialog" aria-labelledby="liquidaModalLabel" aria-hidden="true">
                                                <div class="modal-dialog" role="document">
                                                    <div class="modal-content">
                                                        <div class="modal-header">
                                                            <h5 class="modal-title" id="liquidaModalLabel">Confirma Liquidação</h5>
                                                            <button type="button" class="close" data-dismiss="modal" aria-label="Fechar">
                                                                <span aria-hidden="true">&times;</span>
                                                            </button>
                                                        </div>
                                                        <div class="modal-body">
                                                            <p>A conta não possui saldo suficiente para efetuar o pagamento.</p>
                                                            <p>Deseja continuar mesmo assim?</p>
                                                        </div>
                                                        <div class="modal-footer">
                                                            @Html.ButtonActionSubmit(text: "Confirmar", htmlAttributes: new { id = "basictoast", onclick = "LiquidarCP()" }) 
                                                            <button style="margin-top: -5px;" type="button" class="btn btn-sm btn-warning" data-dismiss="modal">Cancelar</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        }

                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">                
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    @Html.HiddenFor(m => m.CAPA_CD_ID)
                                                    @Html.HiddenFor(m => m.CAPA_DS_DESCRICAO)
                                                    @Html.HiddenFor(m => m.CAPA_DT_COMPETENCIA)
                                                    @Html.HiddenFor(m => m.CAPA_DT_INICIO_PARCELAS)
                                                    @Html.HiddenFor(m => m.CAPA_DT_LANCAMENTO)
                                                    @Html.HiddenFor(m => m.CAPA_DT_VENCIMENTO)
                                                    @Html.HiddenFor(m => m.CAPA_IN_ATIVO)
                                                    @Html.HiddenFor(m => m.CAPA_IN_LIQUIDADA)
                                                    @Html.HiddenFor(m => m.CAPA_IN_PAGA_PARCIAL)
                                                    @Html.HiddenFor(m => m.CAPA_IN_PARCELADA)
                                                    @Html.HiddenFor(m => m.CAPA_IN_PARCELAS)
                                                    @Html.HiddenFor(m => m.CAPA_IN_TIPO_LANCAMENTO)
                                                    @Html.HiddenFor(m => m.CAPA_NM_FAVORECIDO)
                                                    @Html.HiddenFor(m => m.CAPA_NM_FORMA_PAGAMENTO)
                                                    @Html.HiddenFor(m => m.CAPA_NR_DOCUMENTO)
                                                    @Html.HiddenFor(m => m.CAPA_TX_OBSERVACOES)
                                                    @Html.HiddenFor(m => m.CAPA_VL_PARCELADO)
                                                    @Html.HiddenFor(m => m.CECU_CD_ID)
                                                    @Html.HiddenFor(m => m.FORN_CD_ID)
                                                    @Html.HiddenFor(m => m.COBA_CD_ID)
                                                    @Html.HiddenFor(m => m.FOPA_CD_ID)
                                                    @Html.HiddenFor(m => m.PERI_CD_ID)
                                                    @Html.HiddenFor(m => m.PLCO_CD_ID)
                                                    @Html.HiddenFor(m => m.TIFA_CD_ID)
                                                    @Html.HiddenFor(m => m.USUA_CD_ID)
                                                    @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                    @Html.HiddenFor(m => m.CAPA_IN_TIPO_DOC)
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            @if (Model.CAPA_IN_LIQUIDADA == 1)
	                                        {
                                                @Html.HiddenFor(m => m.CAPA_VL_SALDO)
                                                @Html.HiddenFor(m => m.CAPA_VL_PARCIAL)
                                                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">                
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        <div class="input-group">
                                                            Liquidada
                                                            <span class="input-group-btn">
                                                                @Html.CheckBoxFor(m => m.Liquidada, new { @class = "checkbox i-checks" , @readonly="readonly"})
                                                            </span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">                
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Data Liquidação
                                                        @Html.TextBoxFor(m => m.CAPA_DT_LIQUIDACAO, "{0:dd/MM/yyyy}", new { @class = "form-control", autofocus = true , @readonly="readonly" })
                                                    </div>
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Valor Original
                                                        @Html.TextBoxFor(m => m.CAPA_VL_VALOR,  "{0:n2}",new { @class = "form-control", autofocus = true , @readonly="readonly"})
                                                    </div>
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Descontos(-)
                                                        @Html.TextBoxFor(m => m.CAPA_VL_DESCONTO,"{0:n2}", new { @class = "form-control", autofocus = true , @readonly="readonly"})
                                                    </div>
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Juros (+)
                                                        @Html.TextBoxFor(m => m.CAPA_VL_JUROS,"{0:n2}", new { @class = "form-control", autofocus = true , @readonly="readonly"})
                                                    </div>
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Taxas (+)
                                                        @Html.TextBoxFor(m => m.CAPA_VL_TAXAS, "{0:n2}",new { @class = "form-control", autofocus = true , @readonly="readonly"})
                                                    </div>
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Valor Pago (=)
                                                        @Html.TextBoxFor(m => m.CAPA_VL_VALOR_PAGO,"{0:n2}", new { @class = "form-control", autofocus = true , @readonly="readonly"})
                                                    </div>
                                                </div>
                                                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">   
                                                    <div class="col-lg-4 col-md-4 col-xs-12">
                                                        Conta Bancária
                                                        @Html.TextBoxFor(m => m.FORMA_PAGAMENTO.CONTA_BANCO.COBA_NM_NOME, new { @class = "form-control", autofocus = true , @readonly="readonly"})
                                                    </div>
                                                    @if (Model.FORMA_PAGAMENTO.FOPA_IN_CHEQUE == 1)
	                                                {
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            Número do Cheque
                                                            @Html.TextBoxFor(m => m.CAPA_NR_CHEQUE, new { @class = "form-control", autofocus = true , @readonly="readonly"})
                                                        </div>
	                                                }
                                                </div>

                                                <div class="form-group">
                                                    <br />
                                                    <div class="col-lg-12 col-md-12 col-xs-12">
                                                        @Html.ButtonLinkIndex(text: "Visualizar Conta", action: "EditarContaBanco", controller: "ContaPagar", routeValues: new { id = Model.CAPA_CD_ID })
                                                    </div>
                                                </div>
                                            }
                                            else
                                            {
                                                if (Model.CAPA_IN_PARCELADA == 0)
	                                            {
                                                    <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">                
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            Data Liquidação
                                                            @Html.TextBoxFor(m => m.CAPA_DT_LIQUIDACAO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker",  @id = "data5", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.CAPA_DT_LIQUIDACAO, "",new { @style="color:red" })
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-xs-12">
                                                            Conta Bancária
                                                            @Html.TextBoxFor(m => m.FORMA_PAGAMENTO.CONTA_BANCO.COBA_NM_NOME, new { @class = "form-control", autofocus = true , @readonly="readonly"})
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-xs-12">
                                                            Forma de Pagamento
                                                            @Html.TextBoxFor(m => m.FORMA_PAGAMENTO.FOPA_NM_NOME, new { @class = "form-control", autofocus = true , @readonly="readonly"})
                                                        </div>
                                                    </div>
                                                    <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">                
                                                        @if (Model.CAPA_IN_PAGA_PARCIAL == 0)
	                                                    {
                                                            @Html.HiddenFor(m => m.CAPA_VL_PARCIAL)
                                                            @Html.HiddenFor(m => m.CAPA_VL_SALDO)
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Valor Original
                                                                @Html.TextBoxFor(m => m.CAPA_VL_VALOR, "{0:n2}",new { @class = "form-control", autofocus = true, @readonly="readonly", @id = "valor"})
                                                            </div>
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Descontos(-)
                                                                @Html.TextBoxFor(m => m.CAPA_VL_DESCONTO, new { @class = "form-control", autofocus = true, @id = "desc", onkeyup = "sum()"})
                                                                @Html.ValidationMessageFor(m => m.CAPA_VL_DESCONTO, "",new { @style="color:red" })
                                                            </div>
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Juros (+)
                                                                @Html.TextBoxFor(m => m.CAPA_VL_JUROS, new { @class = "form-control", autofocus = true, @id = "juro", onkeyup = "sum()"})
                                                                @Html.ValidationMessageFor(m => m.CAPA_VL_JUROS, "",new { @style="color:red" })
                                                            </div>
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Taxas (+)
                                                                @Html.TextBoxFor(m => m.CAPA_VL_TAXAS,new { @class = "form-control", autofocus = true, @id = "taxa", onkeyup = "sum()"})
                                                                @Html.ValidationMessageFor(m => m.CAPA_VL_TAXAS, "",new { @style="color:red" })
                                                            </div>
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Valor Pago (=)
                                                                @Html.TextBoxFor(m => m.CAPA_VL_VALOR_PAGO, new { @class = "form-control", autofocus = true, @id = "rec"})
                                                                @Html.ValidationMessageFor(m => m.CAPA_VL_VALOR_PAGO, "",new { @style="color:red" })
                                                            </div>
                                                        }
                                                        else
                                                        {
                                                            @Html.HiddenFor(m => m.CAPA_VL_VALOR)
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Valor já Pago
