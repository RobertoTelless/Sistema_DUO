                , data: data
                , async: false
                , success: function (r) {
                    if (r != 1) {
                        $('#btnParcialContainer').hide();
                        $('#btnSubmitParcial').show();
                    }
                    else {
                        $('#btnParcialContainer').show();
                        $('#btnSubmitParcial').hide();
                    }
                }
            });
        }
    </script>

    @if (ViewBag.Message != null)
    {
        <script type="text/javascript">
            window.onload = function () {
                  alert("@ViewBag.Message");
            };
        </script>
    }
}
