                                                            @Html.ValidationMessageFor(m => m.PROD_CD_CODIGO, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            Código de Barras (Número)
                                                            @Html.TextBoxFor(m => m.PROD_NR_BARCODE, new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.PROD_NR_BARCODE, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-6 col-md-6 col-xs-12">
                                                            Nome*
                                                            @Html.TextBoxFor(m => m.PROD_NM_NOME, new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.PROD_NM_NOME, "", new { @style = "color:red" })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row">
                                                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                    @if (ViewBag.Perfil != "ADM" & ViewBag.Perfil != "GER")
                                                    {
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            Preço Venda
                                                            @Html.TextBoxFor(m => m.PROD_VL_PRECO_VENDA, "{0:n2}", new { @class = "form-control", autofocus = true, @readonly = "readonly", @style = "color:green; font-weight:bold" })
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            Preço Promoção
                                                            @Html.TextBoxFor(m => m.PROD_VL_PRECO_PROMOCAO, "{0:n2}", new { @class = "form-control", autofocus = true, @readonly = "readonly", @style = "color:green; font-weight:bold" })
                                                        </div>
                                                    }
                                                </div>

                                                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                    <div class="col-lg-12 col-md-12 col-xs-12">
                                                        Descrição
