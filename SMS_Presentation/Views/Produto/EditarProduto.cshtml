                                                        @Html.HiddenFor(x => x.BalancaPDV)
                                                        @Html.HiddenFor(x => x.BalancaRetaguarda)
                                                        @Html.HiddenFor(x => x.ProdutoTipoCombo)
                                                        @Html.HiddenFor(x => x.ProdutoOpcaoCombo)
                                                        @Html.HiddenFor(x => x.CobrarMaior)
