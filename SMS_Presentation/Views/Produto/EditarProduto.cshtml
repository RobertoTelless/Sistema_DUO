@using Presentation.Extensions
@model SMS_Solution.ViewModels.ProdutoViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}

<style>
    /* Chrome, Safari, Edge, Opera */
    input::-webkit-outer-spin-button,
    input::-webkit-inner-spin-button {
        -webkit-appearance: none;
        margin: 0;
    }

    /* Firefox */
    input[type=number] {
        -moz-appearance: textfield;
    }
</style>

<div class="wrapper wrapper-content animated fadeInRight">

    <div id="prtpModal" class="modal" tabindex="-1" role="dialog">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                @using (Html.BeginForm("EditarPC", "Produto", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                {
                    <div class="modal-header">
                        <h5 class="modal-title">Editar custo</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            @Html.HiddenFor(x => x.PROD_CD_ID)
                            <input id="prtpCd" type="hidden" name="id" />
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                Filial
                                @Html.DropDownListFor(m => m.FILI_CD_ID, (SelectList)ViewBag.Filiais, "Selecionar", new { @class = "form-control", autofocus = true, id = "filialModal" })
                                @Html.ValidationMessageFor(m => m.FILI_CD_ID, "", new { @style = "color:red" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Custo
                                @Html.TextBoxFor(m => m.PROD_VL_CUSTO, new { @class = "form-control", autofocus = true, id = "custoModal", onfocusout = "CalculaPrecoEdicao()" })
                                @Html.ValidationMessageFor(m => m.PROD_VL_CUSTO, "", new { @style = "color:red" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Markup
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.PROD_VL_MARKUP_PADRAO, new { @class = "form-control", autofocus = true, id = "markupModal", onfocusout = "CalculaPrecoEdicao()", min = "1", max = "100" })
                                    @Html.ValidationMessageFor(m => m.PROD_VL_MARKUP_PADRAO, "", new { @style = "color:red" })
                                    <div class="input-group-addon">
                                        %
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Preço (R$)
                                @Html.TextBoxFor(m => m.PRTP_VL_PRECO, new { @class = "form-control", autofocus = true, id = "precoModal" })
                                @Html.ValidationMessageFor(m => m.PRTP_VL_PRECO, "", new { @style = "color:red" })
                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                Preço Promoção (R$)
                                @Html.TextBoxFor(m => m.PROD_VL_PRECO_PROMOCAO, new { @class = "form-control", autofocus = true, id = "precoPromoModal" })
                                @Html.ValidationMessageFor(m => m.PROD_VL_PRECO_PROMOCAO, "", new { @style = "color:red" })
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        @Html.ButtonActionSubmit(text: "Confirma edição", htmlAttributes: new { id = "basictoast" })
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
                    </div>
                }
            </div>
        </div>
    </div>

    <div class="page-title">
        <h3 class="title">Produtos</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaProduto", "Produto")">Produtos</a>
            </li>
            <li class="active">
                <strong>Produtos - Consulta & Edição</strong>
            </li>
        </ol>
        <br />
    </div>

    @*TABS - PRINCIPAL*@
    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <div class="row">
                        <div style="text-align: center;" class="col-lg-12 col-md-12 col-xs-12">
                            <h5>Produto - Edição</h5>
                            <span style="font-weight: 500; font-size: 20px;">@Model.PROD_NM_NOME</span>
                        </div>
                    </div>
                </div>
                <div class="ibox-content">
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                        <div class="form-group">
                            @Html.ButtonLinkIndex(text: "Duplicar o Produto", action: "ClonarProduto", controller: "Produto", buttonClass: "success demo2", routeValues: new { id = Model.PROD_CD_ID })
