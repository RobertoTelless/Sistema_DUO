                                                            @Html.ValidationMessageFor(m => m.UNID_CD_ID, "", new { @style = "color:red" })
                                                        </div>
                                                        @*<div class="col-lg-2 col-md-3 col-xs-12">
                                                            Produto Composto
                                                            <br />
                                                            @Html.CheckBoxFor(m => m.Composto, new { @class = "checkbox i-checks" })
                                                            @Html.ValidationMessageFor(m => m.Composto, "", new { @style = "color:red" })
                                                        </div>*@
