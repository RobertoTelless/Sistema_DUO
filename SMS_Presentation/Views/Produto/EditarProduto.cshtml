                                                        @Html.HiddenFor(m => m.PROD_IN_COMPOSTO)
                                                        @if (!String.IsNullOrEmpty(Model.PROD_AQ_FOTO))
                                                        {
                                                            <img src="@Url.Content(Model.PROD_AQ_FOTO)" style="width: auto; height: auto; max-height: 110px; max-width: 90px;" data-toggle="modal" data-target="#fotoModal" onclick="CriaModal('@Model.PROD_NM_NOME', '@Url.Content(Model.PROD_AQ_FOTO)')">
                                                        }
                                                        else
                                                        {
                                                            <img src="~/Imagens/Base/icone_imagem.jpg" style="width: auto; height: auto; max-height: 110px; max-width: 90px;" data-toggle="modal" data-target="#fotoModal" onclick="CriaModal('@Model.PROD_NM_NOME', '/Images/gallery/10s.jpg')">
                                                        }
                                                        <br />
                                                        <br />
                                                        <br />
                                                        <br />
                                                        <br />
                                                    </div>
                                                </div>
                                                <div class="col-md-11">
                                                    <div class="row">
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            Categoria*
                                                            @Html.DropDownListFor(m => m.CAPR_CD_ID, (SelectList)ViewBag.Tipos, "Selecionar", new { @class = "form-control", autofocus = true, onchange = "FiltrarSubCategoriaProduto($(this).val(), $(this))", id = "catProd" })
                                                            @Html.ValidationMessageFor(m => m.CAPR_CD_ID, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            Subcategoria
