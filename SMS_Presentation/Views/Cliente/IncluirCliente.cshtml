@using Presentation.Extensions
@model SMS_Solution.ViewModels.ClienteViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="page-title">
        <h3 class="title">Clientes</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaCliente", "Cliente")">Clientes</a>
            </li>
            <li class="active">
                <strong>Clientes - Inclusão</strong>
            </li>
        </ol>
        <br />
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Cliente - Inclusão</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    @*@Html.AntiForgeryToken()*@
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "Erros de Crítica Encontrados:", new { @class = "text-danger" })
                        </div>
                    }
                    <div class="tabs-container">
                        @using (Html.BeginForm("IncluirCliente", "Cliente", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                        {

                            <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-2">Anexos</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-3">Endereços</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-4">Contatos</a></li>
                                <li id="liDadosPessoais" class=""><a data-toggle="tab" href="#tab-5">Dados Pessoais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-6">Dados Comerciais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-7">Observações</a></li>
                            </ul>

                            <div class="tab-content">
                                @*Dados Gerais*@
                                <div id="tab-1" class="tab-pane active">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                    @Html.HiddenFor(m => m.CLIE_DT_CADASTRO)
                                                    @Html.HiddenFor(m => m.CLIE_IN_ATIVO)
                                                    Tipo de Pessoa*
                                                    @Html.DropDownListFor(m => m.TIPE_CD_ID, (SelectList)ViewBag.TiposPessoa, "Selecionar", new { @class = "form-control", autofocus = true, @id = "pessoa", onchange = "ValidaDiv($(this).val())" })
                                                    @Html.ValidationMessageFor(m => m.TIPE_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Filial*
                                                    @Html.DropDownListFor(m => m.FILI_CD_ID, (SelectList)ViewBag.Filiais, "Selecionar", new { @class = "form-control", autofocus = true, id = "filGeral"  })
                                                    @Html.ValidationMessageFor(m => m.FILI_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                @*<div class="col-lg-3 col-md-3 col-xs-12">
                                                    Tipo de Contribuinte
                                                    @Html.DropDownListFor(m => m.TICO_CD_ID, (SelectList)ViewBag.TiposContribuinte, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.TICO_CD_ID, "", new { @style = "color:red" })
                                                </div>*@
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Categoria*
                                                    @Html.DropDownListFor(m => m.CACL_CD_ID, (SelectList)ViewBag.Tipos, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CACL_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;" id="PF">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    CPF
                                                    @Html.TextBoxFor(m => m.CLIE_NR_CPF, new { @class = "form-control", @id = "cpf", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NR_CPF, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;" id="PJ">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    CNPJ
                                                    <div class="input-group">
                                                        @Html.TextBoxFor(m => m.CLIE_NR_CNPJ, new { @class = "form-control", @id = "cnpj" })
                                                        @Html.ValidationMessageFor(m => m.CLIE_NR_CNPJ, "", new { @style = "color:red" })
                                                        <span class="input-group-btn">
                                                            <span class="btn btn-success" onclick="PesquisaCNPJ($('#cnpj').val())">Buscar CNPJ</span>
                                                        </span>
                                                    </div>
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Insc. Estadual
                                                    @Html.TextBoxFor(m => m.CLIE_NR_INSCRICAO_ESTADUAL, new { @class = "form-control", autofocus = true, id = "ie" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NR_INSCRICAO_ESTADUAL, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Insc. Municipal
                                                    @Html.TextBoxFor(m => m.CLIE_NR_INSCRICAO_MUNICIPAL, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NR_INSCRICAO_MUNICIPAL, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Situação
                                                    @Html.TextBoxFor(m => m.CLIE_NM_SITUACAO, new { @class = "form-control", autofocus = true, id = "situacao", @readonly = "readonly" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_SITUACAO, "", new { @style = "color:red" })
                                                </div>
                                                @*<div class="col-lg-2 col-md-2 col-xs-12">
                                                    Saldo
                                                    @Html.TextBoxFor(m => m.CLIE_VL_SALDO, "{0:n2}", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_VL_SALDO, "", new { @style = "color:red" })
                                                </div>*@
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-6 col-md-6 col-xs-12" id="RS">
                                                    Razão Social
                                                    @Html.TextBoxFor(m => m.CLIE_NM_RAZAO, new { @class = "form-control", autofocus = true, @id = "razao" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_RAZAO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-6 col-md-6 col-xs-12">
                                                    Nome*
                                                    @Html.TextBoxFor(m => m.CLIE_NM_NOME, new { @class = "form-control", autofocus = true, @id = "nome" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_NOME, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            @*<div class="form-group" style=" margin-left: -15px; margin-right: -15px;" id="DS">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Regime Tributário
                                                    @Html.DropDownListFor(m => m.RETR_CD_ID, (SelectList)ViewBag.Regimes, "Selecionar", new { @class = "form-control", autofocus = true, @id = "rt" })
                                                    @Html.ValidationMessageFor(m => m.RETR_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Inscrição SUFRAMA
                                                    @Html.TextBoxFor(m => m.CLIE_NR_SUFRAMA, new { @class = "form-control", autofocus = true, @id = "is" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NR_SUFRAMA, "", new { @style = "color:red" })
                                                </div>
                                            </div>*@
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px; display: none;" id="QS">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    <hr />
                                                    <h4>Quadro Societário</h4>
                                                    <br />
                                                    <table style="width: 100%;" class="table table-striped table-bordered table-hover dataTables-example" id="tblQs">
                                                        <thead style="background-color:lightblue">
                                                            <tr>
                                                                <th>Qualificação</th>
                                                                <th>Nome</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Imagens - Foto e Anexos*@
                                <div id="tab-2" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div style="margin-left: 15px;" class="col-lg-12 col-md-12 col-xs-12">
                                                <div class="form-group">
                                                    <h4>Logotipo/Foto</h4><input type="file" accept=".jpg, .png, .jpeg, .gif|images/*" name="file" id="imgCliente" class="btn btn-sm btn-warning" multiple />
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <hr />
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Anexos</h4><input class="btn btn-sm btn-warning" id="inputFile" type="file" multiple />
                                                <br />
                                                <div class="form-group">
                                                    <div>
                                                        <hr />
                                                        <table style="width: 100%;" class="table table-striped table-bordered table-hover dataTables-example" id="myFiles">
                                                            <thead style="background-color:lightsteelblue">
                                                                <tr>
                                                                    <td>Nome</td>
                                                                    <td>Tipo</td>
                                                                    <td>Ações</td>
                                                                </tr>
                                                            </thead>
                                                            <tbody></tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Endereços*@
                                <div id="tab-3" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    <h4>Endereço Principal</h4>
                                                    <br />
                                                    CEP
                                                    <div class="input-group">
                                                        @Html.TextBoxFor(m => m.CLIE_NR_CEP, new { @class = "form-control", @id = "cep", autofocus = true })
                                                        @Html.ValidationMessageFor(m => m.CLIE_NR_CEP, "", new { @style = "color:red" })
                                                        <span class="input-group-btn">
                                                            <span class="btn btn-warning btn-sm" id="btnBuscaCep" onclick="PesquisaCep($('#cep').val(), $(this), 1)">Buscar CEP</span>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Endereço
                                                    @Html.TextBoxFor(m => m.CLIE_NM_ENDERECO, new { @class = "form-control", @id = "endereco", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_ENDERECO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-1 col-md-1 col-xs-12">
                                                    Número
                                                    @Html.TextBoxFor(m => m.CLIE_NR_NUMERO, new { @class = "form-control", @id = "nmr", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NR_NUMERO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Complemento
                                                    @Html.TextBoxFor(m => m.CLIE_NM_COMPLEMENTO, new { @class = "form-control", autofocus = true, @id = "complemento" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_COMPLEMENTO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Bairro
                                                    @Html.TextBoxFor(m => m.CLIE_NM_BAIRRO, new { @class = "form-control", @id = "bairro", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_BAIRRO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Cidade
                                                    @Html.TextBoxFor(m => m.CLIE_NM_CIDADE, new { @class = "form-control", @id = "cidade", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_CIDADE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    UF
                                                    @Html.DropDownListFor(m => m.UF_CD_ID, (SelectList)ViewBag.UF, "Selecionar", new { @class = "form-control", @id = "uf", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.UF_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <hr />
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    <h4>Endereço de Entrega</h4>
                                                    <br />
                                                    CEP Entrega
                                                    <div class="input-group">
                                                        @Html.TextBoxFor(m => m.CLIE_NR_CEP_ENTREGA, new { @class = "form-control", @id = "cep1", autofocus = true })
                                                        @Html.ValidationMessageFor(m => m.CLIE_NR_CEP_ENTREGA, "", new { @style = "color:red" })
                                                        <span class="input-group-btn">
                                                            <span class="btn btn-warning btn-sm" id="btnBuscaCepEntrega" onclick="PesquisaCep($('#cep1').val(), $(this), 2)">Buscar CEP</span>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Endereço Entrega
                                                    @Html.TextBoxFor(m => m.CLIE_NM_ENDERECO_ENTREGA, new { @class = "form-control", @id = "endEntrega", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_ENDERECO_ENTREGA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-1 col-md-1 col-xs-12">
                                                    Número
                                                    @Html.TextBoxFor(m => m.CLIE_NR_NUMERO_ENTREGA, new { @class = "form-control", @id = "nmrEntrega", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NR_NUMERO_ENTREGA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Complemento
                                                    @Html.TextBoxFor(m => m.CLIE_NM_COMPLEMENTO_ENTREGA, new { @class = "form-control", autofocus = true, @id = "compEntrega" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_COMPLEMENTO_ENTREGA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Bairro Entrega
                                                    @Html.TextBoxFor(m => m.CLIE_NM_BAIRRO_ENTREGA, new { @class = "form-control", @id = "bairroEntrega", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_BAIRRO_ENTREGA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Cidade Entrega
                                                    @Html.TextBoxFor(m => m.CLIE_NM_CIDADE_ENTREGA, new { @class = "form-control", @id = "cidEntrega", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_CIDADE_ENTREGA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    UF
                                                    @Html.TextBoxFor(m => m.CLIE_SG_UF_ENTREGA, new { @class = "form-control", @id = "ufEntrega", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_SG_UF_ENTREGA, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Contatos*@
                                <div id="tab-4" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    E-Mail*
                                                    @Html.TextBoxFor(m => m.CLIE_NM_EMAIL, new { @class = "form-control", autofocus = true, id = "email" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_EMAIL, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    E-Mail DANFE
                                                    @Html.TextBoxFor(m => m.CLIE_NM_EMAIL_DANFE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_EMAIL_DANFE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Redes Sociais
                                                    @Html.TextBoxFor(m => m.CLIE_NM_REDES_SOCIAIS, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_REDES_SOCIAIS, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Telefone
                                                    @Html.TextBoxFor(m => m.CLIE_NR_TELEFONES, new { @class = "form-control", autofocus = true, @id = "tel" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NR_TELEFONES, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Celular
                                                    @Html.TextBoxFor(m => m.CLIE_NR_CELULAR, new { @class = "form-control", autofocus = true, @id = "cel" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NR_CELULAR, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    WhatsApp
                                                    @Html.TextBoxFor(m => m.CLIE_NR_TELEFONE_ADICIONAL, new { @class = "form-control", autofocus = true, @id = "alt_tel" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NR_TELEFONE_ADICIONAL, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Dados Pessoais*@
                                <div id="tab-5" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Nome do Pai
                                                    @Html.TextBoxFor(m => m.CLIE_NM_PAI, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_PAI, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Nome da Mãe
                                                    @Html.TextBoxFor(m => m.CLIE_NM_MAE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_MAE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data Nascimento
                                                    @Html.TextBoxFor(m => m.CLIE_DT_NASCIMENTO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true, @id = "dn" })
                                                    @Html.ValidationMessageFor(m => m.CLIE_DT_NASCIMENTO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Sexo
                                                    @Html.DropDownListFor(m => m.SEXO_CD_ID, (SelectList)ViewBag.Sexo, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.SEXO_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Naturalidade
                                                    @Html.TextBoxFor(m => m.CLIE_NM_NATURALIDADE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_NM_NATURALIDADE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-1 col-md-1 col-xs-12">
                                                    UF
                                                    @Html.TextBoxFor(m => m.CLIE_SG_NATURALIADE_UF, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.CLIE_SG_NATURALIADE_UF, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Nacionalidade
                                                    @Html.TextBoxFor(m => m.CLIE_NM_NACIONALIDADE, new { @class = "form-control", autofocus = true })
